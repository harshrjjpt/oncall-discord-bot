{
    "name": "discord-oncall-bot",
    "version": "1.0.0",
    "description": "Discord bot to manage on-call and route urgent issues",
    "main": "src/index.js",
    "engines": {
      "node": ">=18"
    },
    "scripts": {
      "start": "node src/index.js",
      "start:dev": "nodemon --watch src src/index.js",
      "migrate": "knex --knexfile knexfile.js migrate:latest",
      "seed": "knex --knexfile knexfile.js seed:run",
      "register-commands": "node src/services/register-commands.js",
      "setup-supabase": "node setup-supabase.js"
    },
    "dependencies": {
      "discord.js": "^14.11.0",
      "dotenv": "^16.1.4",
      "knex": "^3.0.1",
      "sqlite3": "^5.1.6",
      "node-cron": "^3.0.2",
      "winston": "^3.9.0",
      "pg": "^8.11.0",
      "@supabase/supabase-js": "^2.39.0"
    },
    "devDependencies": {
      "nodemon": "^2.0.22"
    }
  }